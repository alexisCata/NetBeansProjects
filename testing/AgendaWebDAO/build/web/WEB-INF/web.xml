<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>ServAddDAO</servlet-name>
        <servlet-class>servlets.ServAddDAO</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServShowDAO</servlet-name>
        <servlet-class>servlets.ServShowDAO</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NewServlet</servlet-name>
        <servlet-class>servlets.NewServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServDeleteDAO</servlet-name>
        <servlet-class>servlets.ServDeleteDAO</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServDelShowDAO</servlet-name>
        <servlet-class>servlets.ServDelShowDAO</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServUpdShowDAO</servlet-name>
        <servlet-class>servlets.ServUpdShowDAO</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServUpdate</servlet-name>
        <servlet-class>servlets.ServUpdate</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServUpdateDAO</servlet-name>
        <servlet-class>servlets.ServUpdateDAO</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ServAddDAO</servlet-name>
        <url-pattern>/ServAddDAO</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServShowDAO</servlet-name>
        <url-pattern>/ServShowDAO</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NewServlet</servlet-name>
        <url-pattern>/NewServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServDeleteDAO</servlet-name>
        <url-pattern>/ServDeleteDAO</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServDelShowDAO</servlet-name>
        <url-pattern>/ServDelShowDAO</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServUpdShowDAO</servlet-name>
        <url-pattern>/ServUpdShowDAO</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServUpdate</servlet-name>
        <url-pattern>/ServUpdate</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServUpdateDAO</servlet-name>
        <url-pattern>/ServUpdateDAO</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
